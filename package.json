{
    "name": "circom-group-rsa-verify",
    "version": "1.0",
    "description": " Zero Knowledge Proof for group signatures using RSA ",
    "main": "index.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "test": "NODE_OPTIONS=--max_old_space_size=56000 mocha -r ts-node/register 'test/**/*.ts'",
        "testgroup": "NODE_OPTIONS=--max_old_space_size=56000 mocha -r ts-node/register 'test/test_group_rsa_pkcs1v15.ts'",
        "testgroup_v2": "NODE_OPTIONS=--max_old_space_size=56000 mocha -r ts-node/register 'test/test_group_rsa_pkcs1v15_v2.ts'",
        "testgroupdynamic": "NODE_OPTIONS=--max_old_space_size=56000 mocha -r ts-node/register 'test/test_group_rsa_pkcs1v15_readfromfile.ts' --timeout 3600000",
        "test_simple_rsa": "NODE_OPTIONS=--max_old_space_size=56000 mocha -r ts-node/register 'test/test_rsa_pkcs1v15.ts' --timeout 3600000",
        "test_simple_rsa_v2": "NODE_OPTIONS=--max_old_space_size=56000 mocha -r ts-node/register 'test/test_rsa_pkcs1v15_v2.ts' --timeout 3600000",
        "generate_inputjson": "NODE_OPTIONS=--max_old_space_size=56000 mocha -r ts-node/register 'test/generate_input_json.ts' --timeout 3600000"
    },
    "keywords": [
        "rsa",
        "circuit",
        "circom",
        "zksnark",
        "zkp-application",
        "group-signature"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tmorais-fctunl/Circom-group-rsa-verify.git"
    },
    "author": "tmorais-fctunl",
    "license": "GPL-3.0",
    "devDependencies": {
        "@noble/secp256k1": "1.3.4",
        "@types/chai": "^4.3.0",
        "@types/expect": "^24.3.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "^22.1.0",
        "chai": "^4.3.4",
        "circom_tester": "0.0.19",
        "mocha": "^9.1.3",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    }
}
